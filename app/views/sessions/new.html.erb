<h1>LOGIN</h1>

<%= form_with(model: @user, url: "/login") do |f| %>
  
        <%= f.label :email %>
        <%= f.email_field :email %>
  
        <%= f.label :password %>
        <%= f.password_field :password %>

    <%= f.submit :signin%>
<% end %>

<%= button_to "Log in with Google", '/auth/google_oauth2'  %>