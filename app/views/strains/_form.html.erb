<%= render partial: 'layouts/error', locals: {object: @strain} %>

<%= form_for(@strain) do |f| %>
    
    <%= f.collection_select :category_id, Category.all, :id, :name %>
    
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :thc_content %>
    <%= f.number_field :thc_content %>

    <%= f.label :description %>
    <%= f.text_area :description %>

    <%= f.submit %>
<% end %>
