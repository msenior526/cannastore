<h1>Are you at least 21 years of age?</h1>
# Two buttons , yes and no
# If yes, user is redirected to strain index
# If no, flash error 
</br>
</br>
</br>
</br>

<%= form_with(url: root_path, method: :get) do |f| %>
  <%= f.radio_button :choice, "yes" %>
  <%= f.label :choice_yes, "YES" %>
  <%= f.radio_button :choice, "no" %>
  <%= f.label :choice_no, "NO" %>
  <%= f.submit "Submit" %>
<% end %>