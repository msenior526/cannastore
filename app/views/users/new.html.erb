<h1>SIGNUP</h1>
<%= form_for :user do |f| %>
    <%= f.label :username %>
    <%= f.text_field :username %>
        </br>
    <%= f.label :email %>
    <%= f.email_field :email %>
        </br>
    <%= f.label :birthday %>
    <%= f.date_field :birthday %>
        </br>
    <%= f.label :password %>
    <%= f.password_field :password %>
        </br>
    <%= f.label :password_confirmation %>
    <%= f.password_field :password_confirmation %>


    <%= f.submit %>
<% end %>

<%= button_to "Log in with Google", '/auth/google_oauth2'  %>